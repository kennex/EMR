<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="patientAction" class="com.api_craft.Patients.action.PatientAction">
		<property name="patientBo" ref="patientBo" />
	</bean>

	<bean id="patientBo" class="com.api_craft.Patients.bo.impl.PatientBoImpl" >
		<property name="patientDAO" ref="patientDAO" />
	</bean>

	<bean id="patientDAO" class="com.api_craft.Patients.dao.impl.PatientDAOImpl" >
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
</beans>
